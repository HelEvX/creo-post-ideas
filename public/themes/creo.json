{
  "name": "Creo (default)",
  "meta": { "brand": "Creo", "version": "1.0" },
  "font-title": "var(--font-title)",
  "font-body": "var(--font-body)",

  "color-primary": "var(--primary-500)",
  "color-primary-dark": "var(--primary-600)",
  "color-primary-darker": "var(--primary-700)",
  "color-primary-light": "var(--primary-400)",
  "color-primary-lighter": "var(--primary-300)",

  "color-secondary": "var(--secondary-500)",
  "color-secondary-dark": "var(--secondary-600)",
  "color-secondary-darker": "var(--secondary-700)",
  "color-secondary-light": "var(--secondary-400)",
  "color-secondary-lighter": "var(--secondary-300)",

  "color-background": "var(--white)",
  "color-surface": "var(--neutral-50)",
  "color-panel": "var(--neutral-100)",
  "color-overlay": "rgba(6, 214, 160, 0.5)",

  "color-title": "var(--secondary-500)",
  "color-subtitle": "var(--secondary-400)",
  "color-text": "var(--neutral-700)",
  "color-text-soft": "var(--neutral-600)",
  "color-text-muted": "var(--neutral-500)",
  "color-inverse-text": "var(--white)",

  "color-header-bg": "var(--secondary-600)",
  "color-footer-bg": "var(--secondary-600)",
  "color-section-dark": "var(--secondary-600)",
  "color-section-dark-alt": "var(--primary-600)",

  "color-heading-on-dark": "var(--white)",
  "color-text-on-dark": "rgba(255, 255, 255, 0.85)",
  "color-icons-on-dark": "rgba(255, 255, 255, 0.65)",

  "color-border-light": "var(--neutral-200)",
  "color-border-medium": "var(--neutral-400)",
  "color-border-dark": "var(--neutral-700)",

  "color-info": "var(--blue-500)",
  "color-warning": "var(--yellow-500)",
  "color-danger": "var(--red-500)",
  "color-success": "var(--green-500)",

  "color-shadow": "rgba(0, 0, 0, 0.06)",
  "color-disabled-bg": "var(--neutral-100)",
  "color-disabled-text": "var(--neutral-500)"
}
